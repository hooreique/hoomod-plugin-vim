    // motion
    {
      "before": ["u"],
      "after": ["k"] // cursor up
    },
    {
      "before": ["e"],
      "after": ["j"] // cursor down
    },
    {
      "before": ["n"],
      "after": ["h"] // cursor left
    },
    {
      "before": ["i"],
      "after": ["l"] // cursor right
    },
    {
      "before": ["U"],
      "after": ["{", "k", "$"] // cursor previous paragraph
    },
    {
      "before": ["E"],
      "after": ["}", "j", "^"] // cursor next paragraph
    },
    {
      "before": ["N"],
      "after": ["g", "e"] // cursor previous word
    },
    {
      "before": ["I"],
      "after": ["w"] // cursor next word
    },
    {
      "before": ["l"],
      "after": ["^"] // cursor first letter in the row
    },
    {
      "before": ["h"],
      "after": ["0"] // cursor first column in the row
    },
    {
      "before": ["o"],
      "after": ["$"] // cursor last column in the row
    },
    {
      "before": ["H"],
      "after": ["g", "g"] // cursor head
    },
    {
      "before": ["O"],
      "after": ["G", "$"] // cursor tail
    },
    {
      "before": ["y"],
      "after": ["`", "`"] // cursor previous
    },
    // scroll
    {
      "before": ["f"],
      "after": ["3", "<C-Y>"] // scroll up
    },
    {
      "before": ["s"],
      "after": ["3", "<C-E>"] // scroll down
    },
    {
      "before": ["F"],
      "after": ["<C-U>"] // scroll up half screen
    },
    {
      "before": ["S"],
      "after": ["<C-D>"] // scroll down half screen
    },
    {
      "before": ["<Space>"],
      "after": ["z", "z"] // scroll current line to middle
    },
    // clipboard
    // {
    //     "before": ["d"],
    //     "after": ["d"] // delete (cut)
    // },
    {
      "before": ["c"],
      "after": ["y"] // yank (copy)
    },
    {
      "before": ["v"],
      "after": ["p"] // put (paste)
    },
    // mode
    {
      "before": ["t"],
      "after": ["a"] // to insert mode
    },
    {
      "before": ["r"],
      "after": ["i"] // to insert mode
    },
    {
      "before": ["T"],
      "after": ["R"] // to insert mode
    },
    {
      "before": ["R"],
      "after": ["O"] // to insert mode
    },
    {
      "before": ["<Enter>"],
      "after": ["o"] // to insert mode
    },
    {
      "before": ["g"],
      "after": ["v"] // to visual mode
    },
    // edit
    {
      "before": ["a"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<BS>"],
      "after": ["d", "h", "i"] // backspace and go to insert mode
    },
    {
      "before": ["<Del>"],
      "after": ["x", "i"] // delete and go to insert mode
    },
    {
      "before": ["<Tab>"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<S-Tab>"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": [":"],
      "after": ["A", ":", "<Esc>"] // append :
    },
    {
      "before": [";"],
      "after": ["A", ";", "<Esc>"] // append ;
    },
    {
      "before": ["{"],
      "after": ["A", "<Space>", "{"] // append {
    },
    {
      "before": ["z"],
      "commands": ["undo"]
    },
    {
      "before": ["x"],
      "commands": ["redo"]
    },
    {
      "before": ["Z"],
      "commands": ["workbench.action.files.revert"]
    },
    // split window
    {
      "before": ["j"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["J"],
      "commands": ["workbench.action.focusNextGroup"]
    },
    // workbench
    {
      "before": ["q"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["p"],
      "commands": ["workbench.action.togglePanel"]
    },
    {
      "before": ["'"],
      "commands": ["actions.find"]
    },
    {
      "before": ["\""],
      "commands": ["workbench.action.showCommands"]
    }
  ],
